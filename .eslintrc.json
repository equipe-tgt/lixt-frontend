{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "eol-last": ["error", "always"],
    "no-var": "error",
    "prefer-const": "error",
    "no-trailing-spaces": "error",
    "no-multi-spaces": "error"
  }
}
