{
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start-tunnel": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --verbose --coverage",
    "test:silent": "jest --silent --verbose --coverage",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "prettier --write . && eslint --cache --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js}": "eslint --cache --fix",
    "*.{js, md}": "prettier --write"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "collectCoverageFrom": [
      "**/src/screens/**"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^6.0.6",
    "@react-navigation/core": "^6.0.2",
    "@react-navigation/native": "^6.0.3",
    "@react-navigation/routers": "^5.7.4",
    "@react-navigation/stack": "^6.0.8",
    "axios": "^0.21.4",
    "date-fns": "^2.23.0",
    "expo": "~41.0.1",
    "expo-barcode-scanner": "~10.1.2",
    "expo-localization": "~10.1.0",
    "expo-location": "~12.0.4",
    "expo-status-bar": "~1.0.4",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "i18next": "^20.3.2",
    "moment": "^2.29.1",
    "native-base": "^3.0.2",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-i18next": "^11.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-calendar-picker": "^7.1.2",
    "react-native-chart-kit": "^6.11.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-input-spinner": "^1.7.11",
    "react-native-masked-text": "^1.13.0",
    "react-native-month-selector": "^1.4.0",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "^3.0.0",
    "react-native-svg": "^12.1.0",
    "react-native-web": "^0.17.5",
    "react-navigation": "^4.4.4",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.1.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "jest": "26.6.3",
    "jest-dom": "^4.0.0",
    "jest-expo": "^42.1.0",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2"
  }
}
